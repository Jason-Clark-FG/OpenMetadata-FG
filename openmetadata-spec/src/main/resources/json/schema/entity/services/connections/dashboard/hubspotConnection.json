{
  "$id": "https://open-metadata.org/schema/entity/services/connections/dashboard/hubspotConnection.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HubspotConnection",
  "description": "HubSpot Dashboard Connection Config",
  "type": "object",
  "javaType": "org.openmetadata.schema.services.connections.dashboard.HubspotConnection",
  "definitions": {
    "hubspotType": {
      "description": "HubSpot service type",
      "type": "string",
      "enum": ["Hubspot"],
      "default": "Hubspot"
    }
  },
  "properties": {
    "type": {
      "title": "Service Type",
      "description": "Service Type",
      "$ref": "#/definitions/hubspotType",
      "default": "Hubspot"
    },
    "accessToken": {
      "title": "Access Token",
      "description": "HubSpot Private App Access Token or OAuth Access Token",
      "type": "string",
      "format": "password"
    },
    "hubId": {
      "title": "Hub ID",
      "description": "HubSpot Hub ID (optional)",
      "type": "string"
    },
    "supportsMetadataExtraction": {
      "$ref": "../connectionBasicType.json#/definitions/supportsMetadataExtraction"
    }
  },
  "additionalProperties": false,
  "required": ["accessToken"]
}